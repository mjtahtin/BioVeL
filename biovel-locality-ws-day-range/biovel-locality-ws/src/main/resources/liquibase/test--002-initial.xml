    </addColumn>
    <addColumn tableName="location">
            <column name="latitude" type="varchar(256)"/>   
    </addColumn>
    <addColumn tableName="location">
            <column name="longitude" type="varchar(256)"/>   
    </addColumn>
     
  </changeSet>
</databaseChangeLog>